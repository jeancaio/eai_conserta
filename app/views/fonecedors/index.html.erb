<p id="notice"><%= notice %></p>

<h1>Fonecedors</h1>

<table class="table bordered">
  <thead>
    <tr>
      <th>Nome</th>
      <th>Tipo servico</th>
      <th>Endereco</th>
      <th>Descricao</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fonecedors.each do |fonecedor| %>
      <tr>
        <td><%= fonecedor.nome %></td>
        <td><%= fonecedor.tipo_servico %></td>
        <td><%= fonecedor.endereco %></td>
        <td><%= fonecedor.descricao %></td>
        <td><%= link_to 'Show', fonecedor %></td>
        <td><%= link_to 'Edit', edit_fonecedor_path(fonecedor) %></td>
        <td><%= link_to 'Destroy', fonecedor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fonecedor', new_fonecedor_path %>
